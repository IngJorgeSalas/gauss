<restlet scriptid="{scriptid}">
    <description>Script Record {file}</description>
    <isinactive>F</isinactive>
    <!--El nombre del userevent es obligatorio -->
    <name>{name}</name>
    <notifyowner>T</notifyowner>
    <!-- Ruta al archivo JS para crear el script en NetSuite [/SuiteScripts/path/to/file.js]   -->
    <scriptfile>[/SuiteScripts/{file}]</scriptfile>
    <scriptdeployments>
        <!-- Ruta al archivo JS para crear el script en NetSuite [/SuiteScripts/path/to/file.js]   -->
        <scriptdeployment scriptid="customdeploy_{deploy}">
            <allemployees>F</allemployees>
            <allpartners>F</allpartners>
            <allroles>F</allroles>
            <audslctrole></audslctrole>
            <eventtype></eventtype>
            <isdeployed>T</isdeployed>
            <isonline>F</isonline>
            <loglevel>DEBUG</loglevel>
            <runasrole></runasrole>
            <status>TESTING</status>
            <title>sup yo</title>
        </scriptdeployment>
    </scriptdeployments>
</restlet>